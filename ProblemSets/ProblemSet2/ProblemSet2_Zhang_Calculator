def sum1(x, y):
    summation = x + y
    sum1
    return summation

from math import sqrt
import numpy as np
from scipy import linalg as la
import matplotlib.pyplot as plt

def prod(x, y):
    production = x * y
    prod
    return production

def sqr(x):
    squareroot = sqrt(x)
    sqr
    return squareroot
